{
  "name": "MarkdownPanel",
  "sencha": {
    "namespace": "MdPanel",
    "type": "code",
    "toolkit": "classic",
    "creator": "Johan van de Merwe",
    "summary": "Panel that supports Remarkable.js Markdown (view only)",
    "detailedDescription": "Panel that supports Remarkable.js Markdown (view only)",
    "version": "1.0.0",
    "compatVersion": "1.0.0",
    "format": "1",
    "slicer": {
      "js": [
        {
          "path": "${package.dir}/sass/example/custom.js",
          "isWidgetManifest": true
        }
      ]
    },
    "output": "${package.dir}/build",
    "local": true,
    "sass": {
      "namespace": "MarkdownPanel",
      "etc": [
        "${package.dir}/sass/etc/all.scss"
      ],
      "var": [
        "${package.dir}/sass/var"
      ],
      "src": [
        "${package.dir}/sass/src/all.scss"
      ]
    },
    "classpath": [
      "${package.dir}/src"
    ],
    "overrides": [
      "${package.dir}/overrides"
    ],
    "example": {
      "path": [
        "${package.dir}/examples"
      ]
    },
    "resources": [
      {
        "path": "resources",
        "output": "shared"
      }
    ],
    "requires": []
  }
}